{
  "name": "resin-docker-aws-iot-demo",
  "description": "AWS IoT messaging from a Resin.io Docker container",
  "version": "1.1.0",
  "homepage": "https://github.com/thegreatsunra/resin-docker-aws-iot-demo",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegreatsunra/resin-docker-aws-iot-demo.git"
  },
  },
  "scripts": {
    "lint": "eslint --ext .js ./",
    "start": "node ./index.js -d --host-name=xxxxxxxxxxxxxx.iot.xx-xxxx-x.amazonaws.com --certificate-dir=./cert --private-key=private.pem.key --client-certificate=certificate.pem.crt --ca-certificate=root-CA.crt"
  },
  "main": "index.js",
  "keywords": [
    "resin.io",
    "webserver",
    "nodejs",
    "expressjs"
